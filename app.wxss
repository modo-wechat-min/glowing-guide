 /* 0等待确认，1预定未付，2预定已付，3消费中，4已结帐，5订单关闭，6客户取消  */
 .icon-cancel{
   color: #333;
 }
.icon-success{
  color: #07c904;
}
.icon-wait{
  color: #ff6644;
}
.icon-waitpay{
  color: #fc8702;
}
.icon-ing{
  color: #2dbaf1;
}
.icon-close{
  color:#047fff;
}
/*警示状态栏*/
.status-bar{
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    height: 90rpx;
    border-top: solid #efe5a7 3rpx;
    background-color: #fffae2;
    font-size: 30rpx;
    color: #222;
}
.status-bar text{
    padding: 0 3px;
    color: #e61773;
}
.in-block{
  display: inline-block;
}
.yuan{
  font-size: 20rpx;
  color: #ff0000;
}
.price{
  font-size: 44rpx;
}
.float_r{
  float: right;
}
.flexone{
  display: flex;
  justify-content: flex-start;
}
.flextwo{
  display: flex;
  justify-content: space-between;
}
.ellipsis{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.shadow{
  box-shadow: 4rpx 0 8rpx rgba(0, 0, 0, 0.1),
                -4rpx 0 8rpx rgba(0, 0, 0, 0.1),
                0rpx 4rpx 8rpx rgba(0, 0, 0, 0.1),
                0rpx -4rpx 8rpx rgba(0, 0, 0, 0.1);
}
.relative{
  position: relative;
}
.btn-active{
background: #bd362f!important;
color: #fff!important;
}
