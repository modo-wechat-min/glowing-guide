.flextwo{
  position: relative;
  padding: 30rpx;
}
.flextwo::after{
  content: "";
  width: 14rpx;
  height: 14rpx;
  border-top: 1rpx solid #999;
  border-right: 1rpx solid #999;
  transform: rotate(45deg);
  position: absolute;
  right: 30rpx;
  top: 60rpx;
}
.border::after{
  display: none;
}
