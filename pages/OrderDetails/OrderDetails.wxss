.main-box{
  padding-bottom: 116rpx;
}
.status{
  line-height: 90rpx;
  font-size: 32rpx;
  color: #ff6644;
}
.order-header{
  padding: 20rpx 0;
  line-height: 40rpx;
  color: #666;
}
.order-header .font24{
  line-height: 40rpx;
}
.order-img{
  width: 100%;
  border-radius: 8rpx;
  height: 426rpx;
}
.img-box{
  font-size: 34rpx;
  line-break: 50rpx;
  padding-bottom: 36rpx;
}
.branch{
  margin: 10rpx 0 4rpx;
}
.box-container{
  padding: 36rpx 0;
  font-size: 24rpx;
  color: #666;
}
.box-title{
  font-size: 30rpx;
color: #333;
}
.box-item{
  margin-top: 16rpx;
}
.btn{
  position: fixed;
  width: 100%;
  height: 116rpx;
  background: #ff6644;
  bottom: 0;
  left: 0;
  color: #fff;
  line-height: 116rpx;
  border-radius: 0;
}
.quick-box{
  text-align: center;
}
.inbock{
  display: inline-block;
  margin: 0 40rpx;
}
.phone{
  height: 22rpx;
  width: 22rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
}
.delete{
  width: 24rpx;
  height: 28rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
  vertical-align: middle;
  margin-top: -8rpx;
}
.down-box{
  text-align: center;
  margin-top: 20rpx;
  color: #333;
}
.icon-down,.icon-up{
  width: 14rpx;
  height: 14rpx;
  border-bottom: 1rpx solid #333;
  border-right: 1rpx solid #333;
  transform: rotate(45deg);
  margin: 0 auto;
  vertical-align: middle;
  margin-top: -12rpx;
}
.icon-up{
  transform: rotate(225deg);
  margin-top: 6rpx;
}
.disable-class{
  background: #eee; 
  color: #333;
}


.unsubscribe-title {
  position: absolute;
  left: 0%;
  margin-left: -150rpx;
  width: 300rpx;
  text-align: center;
  top: 20rpx;
  font-size: 22rpx;
  color: #4c4c4c;
}

.unsubscribe-rule {
  position: absolute;
  left: 50%;
  margin-left: -88rpx;
  width: 176rpx;
  text-align: center;
  bottom: 80rpx;
  word-break: break-all;
  word-wrap: break-word;
  background: #f8f8f8;
  font-size: 20rpx;
  padding: 12rpx 0;
}

.showmian {
  background: transparent;
  margin: 0 auto;
  width: 100%;
  min-height: 100rpx;
  overflow: visible;
}

.one-item {
  text-align: center;
  font-size: 30rpx;
  color: #646464;
  width: 276rpx;
  display: inline-block;
  border-top: 1rpx solid #444;
  height: 50rpx;
  position: relative;
  overflow: visible;
  margin-top: 120rpx;
}

.one-item:first-of-type { 
  margin-left: 7%;
  width: 224rpx;
}

.one-item:last-of-type {
  width: 0;
}

.rule-icon {
  position: absolute;
  left: -15rpx;
  top: -15rpx;
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
}

.rule-icon.active {
  background: url(data:image/png;base64,);
  background-size: cover;
}
.unsubscribe-box{
  padding: 36rpx 0;
}
.order-cancle{
  display:inline-block;
background:transparent!important;
font-size:24rpx;
color:#999;
height:26rpx;
line-height:24rpx;
margin-bottom: -1rpx;
}
.order-cancle::after { 
  border: 0;
} 