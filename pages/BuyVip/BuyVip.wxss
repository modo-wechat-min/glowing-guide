.vip-name{
  height: 70rpx;
  margin-top: 16rpx;
  font-size: 32rpx;
    font-weight: 600;
}
.right-item{
  padding: 22rpx 26rpx 22rpx 90rpx;
  background: #f5f5f5;
  box-shadow: 0 0 2rpx 4rpx rgba(0, 0, 0, 0.06);
  border-radius: 6rpx;
}
.right-item-title{
  font-size: 26rpx;
  margin-bottom: 40rpx;
  position: relative;
  font-weight: 600;
}
.right-item-title::before,.right-item-title::after{
  display: inline-block;
  content: "";
position: absolute;

}
.right-item-title::before{

width: 42rpx;
height: 42rpx;
background: #333;
border-radius: 21rpx;
left: -62rpx;
top: -4rpx;
}
.right-item-title::after{
width: 27rpx;
height: 29rpx;
background: url(data:image/png;base64,);
left: -52rpx;
top: 2rpx;
background-size: cover;

}
.right-item-content{
  font-size: 24rpx;
  line-height: 42rpx;
}



.img-slider{
  /* padding-top: 70rpx; */
}
 .dots{
  left: 0;
  right: 0;
  bottom: 20rpx;
  display: flex;
  justify-content: center;
  margin: 30rpx 0 22rpx;
}
 .dots .dot{
  margin: 0 8rpx;
  width: 14rpx;
  height: 14rpx;
  background: #ffdcd4;
  border-radius: 8rpx;
  transition: all .6s;
}
.dots .dot.active{
  background: #ffa38f;
}

.swiper-box{
  height: 230rpx;
}
.swiper-item{
  position: relative;
height: 100%;
  text-align: center;
  box-sizing: border-box;
}
.swiper-img{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: -1;
  left: 0;
}
.item-title{
  font-size: 28rpx;
  font-weight: 600;
  height: 80rpx;
  line-height: 80rpx;
  /* margin-top: 6rpx; */
}
.item-discrible{
  font-size: 22rpx;
  font-weight: 600;
}
.item-right{
  font-size: 20rpx;
  line-height: 40rpx;
margin-top: 8rpx;
}
.update-btn{
  padding: 2rpx;
  border: 2rpx solid #333; 
  display: inline-block;
}
.update-btn view{
  width: 128rpx;
  height: 42rpx;
  background: #333;
