.box {
  padding: 52rpx 30rpx;
}

.bg-img {
  width: 390rpx;
  height: 145rpx;
}

#phone {
  margin: 136rpx auto 60rpx;
}

.phone, .code {
  border-bottom: 1rpx solid #333; 
  color: #858585;
  font-size: 30rpx;
  height: 52rpx;
  line-height: 52rpx;
}

.input-title {
  font-size: 28rpx;
  color: #999;
  margin-bottom: 4rpx;
}

#code {
  position: relative;
}

.send {
  position: absolute;
  right: 0;
  top: 52rpx;
  font-size: 26rpx;
  color: #333;
  z-index: 99;
}
.login{
  width: 566rpx;
  height: 82rpx;
  background: #ff6644;
  color: #fff;
  line-height: 82rpx;
  margin-top: 196rpx;
  font-size: 36rpx;
  border-radius: 0;
  box-shadow: 3px 3px 6px rgba(255, 102, 68, 0.4),-3px 3px 6px rgba(255, 102, 68, 0.4);
}
.note{
  font-size: 22rpx;
  color: #999;
  text-align: center;
  display: inline-block;
  width: 100%;
  margin-top: 40rpx;
  line-height: 34rpx;
}

/* 信息完善 */
.fix-info{
  position: absolute;
  z-index: 999;
  width: 100%;
  height: 100%;
  background: #fff;
  left: 100%;
  top: 0;
  text-align: center;
  padding-top: 136rpx;
   padding: 60rpx 66rpx;
  box-sizing: border-box;
  
}

.man,.femail{
  
  width: 300rpx;
  height: 300rpx;
  border: 1rpx solid transparent;
  font-size: 30rpx;
  margin-top: 66rpx;
  position: relative;
  box-sizing: border-box; 
}
.man.active{
   border: 1rpx solid #36b1ff;
}
.femail.active{
   border: 1rpx solid #ff6dc0;
}
.chose{
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 16rpx;
}
.chose::after{
width: 26rpx;
height: 26rpx;
background: url(data:image/png;base64,);
position: absolute;
left: 50%;
top: 50%;
margin-left: -13rpx;
margin-top: -13rpx;
content: "";
z-index: 999;
background-size: cover;
}
.chose-man{
  background: #36b1ff;
  right: 16rpx;
  
}
.chose-fe{
  background: #ff6dc0;
  left: 16rpx;
}

.icont{
  display: inline-block;
  background: url("https://www.modo-life.com/Images/WeChatMiniStatic/sex_icon.png");
  overflow: hidden;
  margin: auto;
  
}
.sex-icon{
  width: 154rpx;
  height: 235rpx;
  background-size: 349rpx 301rpx;
  margin: 10rpx 0 0rpx;
  
  
}
.man-icon{
  background-position: -185rpx -10rpx;
  margin-left: -30rpx;
}
.femail-icon{
   background-position:  -10rpx -10rpx;
   margin-left: 30rpx;
}
.info-box{
  text-align: left;
  padding-top: 30rpx;
}
.info-box>input,.info-birthday{
  border: 1rpx solid #ddd;
  height: 80rpx;
  margin-top: 30rpx;
  font-size: 30rpx;
  line-height: 80rpx;
  position: relative;
  padding-left: 108rpx;
  width: 618rpx;
  box-sizing: border-box;
  color: #666;
}
.info-name::before,.info-birthday::before{
position: absolute;
content: "";
display: inline-block;
width: 38rpx;
height: 38rpx;
left: 30rpx;
top: 21rpx;
}
.info-name::before{
background: url(data:image/png;base64,);
background-size: cover;
}
.info-birthday::before{
  background: url(data:image/png;base64,);
background-size: cover;
}
.btn-type0{
  margin-top: 60rpx;
}
.skip{
  font-size: 30rpx;
  margin-top: 60rpx;
  float: right;
}
.skip-active{
  color: blue;
}